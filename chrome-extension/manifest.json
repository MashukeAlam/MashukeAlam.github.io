{
    "manifest_version" : 2,
    "name" : "Jim", 
    "version" : "3.0", 
    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],

            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts" : ["background.js"]
    },
    "permissions" : ["tabs"],
    "browser_action": {
        "default_icon" : "icons8-gamma-48.png",
        "default_popup" : "sketch/popup.html", 
        "default_title" : "Alhamdulillah !!"
    }
}